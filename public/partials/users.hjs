<!-- User Window -->
    <div class="window float" id="users">
        <form id="frmUser">

        <fieldset class="entry">
        <legend>Specialty Graphics Employee Entry Window</legend>
        <div class="close">[<a onClick="hideMe('users');">X</a>]</div>
        <ul>
            <li><label>Salesperson</label>    
                <input type=text id='salespersn'>
                <label>Start Date</label>
                <input type="text" id="startDt">
            </li>
        </ul>
        
        <div class="group">
        <ul>
            <li><label>F Name</label>
                <input type="text" id="firstName">
                <label>Status</label>
                <input type="text" id="status">
            </li>
            <li><label>L Name</label>
                <input type="text" id="company">
                <label>SS Num</label>
                <input type="text" id="ssn" size="2">
            </li>
            <li><label>Address</label><input type="text" id="address1">
             <label>B Date</label>
                <input type="text" id="dob">
