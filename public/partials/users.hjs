                <label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</label>
                <input type="text" id="address2">
                <label>M stat</label>
                <input type="text" id="married">
               
            </li>
            <li><label>City</label>
                <input type="text" id="city" size="10">
                <label>State</label>
                <input type="text" id="state" size="1">
                <label>Zip</label>
                <input type="text" id="zip" size="2">
            </li>
        </ul>
        </div>
        <ul class="left">
            <li><label>Weekly Salary</label>    
                <input type=text id='salary'>
                <label>Office</label>
                <input type="text" id="fax">
            </li>
            <li></li>
            <li><label>Team Leaders</label>
                <input type="text" id="sec">
                <label>Level</label>
                <input type="text" id="lastStat">
            </li>
        </ul>
        </fieldset>
        </form>
    </div>


</div>